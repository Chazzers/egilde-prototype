<!DOCTYPE html>
<html lang="nl">
<head>
	<%- include('partials/Head.ejs') %> 
</head>
<body class="omahaPage">
	<header class="indexheader">
		<div class="titlecontainer">
			<h1>OMAHA filter</h1>
		</div>
		<div class="buttons">
			<%- include('partials/HeaderNav.ejs') %>
		</div>
	</header>
	<main>
		<div class="steps">
			<article>1</article>
			<article>2</article>
		</div>
		<h2>
			Kies een van de domeinen om op te filteren
		</h2>
		<section class="omaha">
			<% filterData.forEach(item => { %>
				<div>
				<a 
					class="omaha-domein-label" 
					href="/omaha-filter/<%= item.id %>"
				>
				<img src="<%= item.img %>" alt="">
				</a>
				<p><%= item.domein %></p>
			</div>
			<% }) %>
		</section>
			
	</main>
</body>
</html>